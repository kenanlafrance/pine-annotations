{
"name": "kenanlafrance/pine-annotations",
"description": "Fork of larachimp/pine-annotations: Easing annotations reading in Laravel.",
"type": "library",
"keywords": ["php", "Laravel", "annotations", "reader"],
"homepage": "https://github.com/kenanlafrance/pine-annotations",
"license": "MIT",
"authors": [
    {
        "name": "Kenan Lafrance",
        "email": "your-email@example.com",
        "role": "Fork Maintainer"
    },
    {
        "name": "Percy Mamedy",
        "email": "percymamedy@gmail.com",
        "role": "Original Author"
    }
],
"require": {
    "php": "^7.3|^8.0",
    "doctrine/annotations": "^1.13",
    "doctrine/cache": "^2.1",
    "illuminate/support": "^9.0",
    "symfony/finder": "^6.0"
},
"require-dev": {
    "mockery/mockery": "^1.4.2",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3.3"
},
"autoload": {
    "psr-4": {
        "Kenanl\\PineAnnotations\\": "src/"
    }
},
"autoload-dev": {
    "psr-4": {
        "Kenanl\\PineAnnotations\\Tests\\": "tests/"
    }
},
"extra": {
    "laravel": {
        "providers": [
            "Kenanl\\PineAnnotations\\PineAnnotationsServiceProvider"
        ],
        "aliases": {
            "AnnotationsReader": "Kenanl\\PineAnnotations\\Facades\\Reader"
        }
    }
},
"minimum-stability": "dev",
"prefer-stable": true
}
